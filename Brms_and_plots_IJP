library(readr)
library(ggplot2)
library(conflicted)
library(dplyr)
conflicts_prefer(dplyr::filter)
library(brms)
conflicts_prefer(brms::ar)
conflicts_prefer(stats::lag)


csv <- read_csv("~/Desktop/Jan2024_Morph.csv")
View(csv)

######## Redoing PFG and Diversity Stats without M8 - M10 ########

### Diversity ###

hist(csv$Div_New)
#slightly left skewed
mean(csv$Div_New)
# [1] 1.935
var(csv$Div_New)
# [1] 0.4128392
## Underdispersion and a pretty normal distribution, may have to use Poisson ##

# age, sex, rank, group, season #
Div1 <-brm(Div_New ~ AgeY + (1|Monkey), data = csv,
           family = poisson(),
           seed = 1,
           control = list(adapt_delta = 0.9))
summary(Div1)
plot(Div1)
pp_check(Div1)
#pp_check doesn't look great actually....
#Population-Level Effects: 
#Estimate Est.Error l-95% CI u-95% CI Rhat Bulk_ESS Tail_ESS
#Intercept     0.76      0.13     0.50     1.01 1.00     8585     2621
#AgeY         -0.01      0.01    -0.03     0.01 1.00     8601     2645

Div2 <-brm(Div_New ~ Sex + (1|Monkey), data = csv,
           family = poisson(),
           seed = 1)
summary(Div2)
plot(Div2)
pp_check(Div2)
#Population-Level Effects: 
#Estimate Est.Error l-95% CI u-95% CI Rhat Bulk_ESS Tail_ESS
#Intercept     0.67      0.07     0.54     0.80 1.00     8548     3188
#SexMale      -0.04      0.11    -0.25     0.18 1.00     7827     2990
#SexUNK       -0.12      0.32    -0.78     0.46 1.00     5821     2703

Div3 <-brm(Div_New ~ Rank + (1|Monkey), data = csv,
           family = poisson(),
           seed = 1)
summary(Div3)
plot(Div3)
pp_check(Div3)
#Population-Level Effects: 
#Estimate Est.Error l-95% CI u-95% CI Rhat Bulk_ESS Tail_ESS
#Intercept     0.63      0.12     0.38     0.86 1.00     4967     2876
#Rank          0.01      0.03    -0.05     0.06 1.00     5231     3139


