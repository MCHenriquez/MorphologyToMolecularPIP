SteggPFG1 <-brm(SteggPFG ~ AgeY + (1|Monkey),data = csv,
               family = negbinomial(),
               seed = 1)
summary(SteggPFG1)
plot(SteggPFG1)
# Population-Level Effects: 
#             Estimate Est.Error l-95% CI u-95% CI Rhat Bulk_ESS Tail_ESS
# Intercept     2.51      0.36     1.82     3.20 1.00     3543     2804
# AgeY         -0.08      0.03    -0.13    -0.03 1.00     3884     2852
# significant! 
pp_check(SteggPFG1)
