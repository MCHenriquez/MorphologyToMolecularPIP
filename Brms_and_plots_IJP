mean(csv$FilaPFG)
# [1] 6.47
var(csv$FilaPFG)
# [1] 202.9941

FilaPFG1 <-brm(FilaPFG ~ AgeY + (1|Monkey),data = csv,
           family = negbinomial(),
           seed = 1)
summary(FilaPFG1)
plot(FilaPFG1)
# Population-Level Effects: 
#             Estimate Est.Error l-95% CI u-95% CI Rhat Bulk_ESS Tail_ESS
# AgeY          0.06      0.02     0.02     0.10 1.00     3777     3056

FilaPFG2 <-brm(FilaPFG ~ Sex + (1|Monkey),data = csv,
           family = negbinomial(),
           seed = 1)
summary(FilaPFG2)
