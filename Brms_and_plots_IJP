###### creating plots for significant results chapter 1 #######

library(tidybayes)

#### PPG V AGE ####
PPG1 <-brm(PPG ~ AgeY + (1|Monkey),data = csv,
              family = negbinomial(),
              seed = 1)
summary(PPG1)
plot(PPG1)

# Calculate the marginal effects
PPG1plot <- conditional_effects(PPG1, effects = "AgeY")
# Plot the marginal effects
plot(PPG1plot, 
     line_args = list(color = 'black'),
     points = T,
     point_args = list(color='darkgreen', size=1))

#### FilaPFG V Age ####

FilaPFG1 <-brm(FilaPFG ~ AgeY + (1|Monkey),data = csv,
               family = negbinomial(),
               seed = 1)
summary(FilaPFG1)
plot(FilaPFG1)

# Calculate the marginal effects
FilaPPG1plot <- conditional_effects(FilaPFG1, effects = "AgeY")
# Plot the marginal effects
plot(FilaPPG1plot, 
     line_args = list(color = 'black'),
     points = T,
     point_args = list(color='darkseagreen4', size=1))

#### Stegg PA v Age ####

SteggPA1 <-brm(SteggPA ~ AgeY + (1|Monkey),data = csv,
               family = bernoulli(link = "logit"),
               seed = 1)
summary(SteggPA1)
plot(SteggPA1)

# Calculate the marginal effects
SteggPAplot <- conditional_effects(SteggPA1, effects = "AgeY")
# Plot the marginal effects
plot(SteggPAplot, 
     line_args = list(color = 'black'),
     points = T,
     point_args = list(color='forestgreen', size=1))

#### Stegg PFG V Age ####
SteggPFG1 <-brm(SteggPFG ~ AgeY + (1|Monkey),data = csv,
                family = negbinomial(),
                seed = 1)
summary(SteggPFG1)
plot(SteggPFG1)


# Calculate the marginal effects
SteggPFGplot <- conditional_effects(SteggPFG1, effects = "AgeY")
# Plot the marginal effects
plot(SteggPFGplot, 
     line_args = list(color = 'black'),
     points = T,
     point_args = list(color='olivedrab', size=1))

#### PFG V Season ####
PPG15 <-brm(PPG ~ Season + (1|Monkey),data = csv,
            family = negbinomial(),
            seed = 1)
summary(PPG15)
plot(PPG15)

